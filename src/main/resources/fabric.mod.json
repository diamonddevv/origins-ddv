{
  "schemaVersion": 1,
  "id": "ddvorigins",
  "version": "${version}",

  "name": "DDV Origins",
  "description": "Origins Side Mod by DiamondDev, adding some origins and some utility things.",
  "authors": [
    "DiamondDev"
  ],
  "contact": {
    "homepage": "https://diamonddevv.github.io/",
    "sources": "https://github.com/diamonddevv/origins-ddv"
  },

  "license": "CC0-1.0",
  "icon": "assets/ddvorigins/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.diamonddev.ddvorigins.DDVOrigins"
    ],
    "client": [
      "net.diamonddev.ddvorigins.DDVOriginsClient"
    ],
    "libgenetics": [
      "net.diamonddev.ddvorigins.impl.LibGeneticsApiImpl"
    ],
    "cardinal-components-entity": [
      "net.diamonddev.ddvorigins.impl.CCAEntityInitializerImpl"
    ]
  },
  "mixins": [
    "ddvorigins.mixins.json"
  ],

  "custom": {
    "modmenu": {
      "links": {
        "ddvorigins.modmenu.microwave": "https://i.kym-cdn.com/entries/icons/original/000/029/618/cover7.jpg"
      }
    },

    "cardinal-components": [
      "ddvorigins:chronokinesis"
    ]
  },

  "depends": {
    "fabricloader": ">=0.14.6",
    "fabric": "*",
    "minecraft": "~1.20",
    "java": ">=17",
    "origins": ">=1.8.1",
    "libgenetics": ">=1.0.0"
  },
  "suggests": {

  }
}
